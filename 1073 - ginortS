# Enter your code here. Read input from STDIN. Print output to STDOUT
s = input()

def func_for_alph(x):
    if x.islower():
        return x.upper()
    elif x.isupper():
        return x.lower()


alph = ''.join(sorted((x for x in s if not x.isnumeric()), key=func_for_alph))
od = ''.join(sorted(x for x in s if x.isnumeric() and int(x) % 2 != 0))
ev = ''.join(sorted(x for x in s if x.isnumeric() and int(x) % 2 == 0))

print(alph+od+ev)


